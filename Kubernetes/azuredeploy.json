{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "deploymentName": {
        "type": "string"
      }
    },
    "variables": {
        "templateLink": "https://raw.githubusercontent.com/neumanndaniel/devtestlab/master/Kubernetes/AKS.json"
    },
    "resources": [
        {
            "apiVersion": "[providers('Microsoft.Resources','deployments').apiVersions[0]]",
            "type": "Microsoft.Resources/deployments",
            "name": "[parameters('deploymentName')]",
            "properties": {
              "mode": "Incremental",
              "templateLink": {
                "uri": "[variables('templateLink')]",
                "contentVersion": "1.0.0.0"
              },
              "parameters": {
                "servicePrincipalClientId": {
                    "reference": {
                        "keyVault": {
                          "id": "/subscriptions/05499249-2bf5-4767-b121-db90943e363e/resourceGroups/kubernetesrg921115/providers/Microsoft.KeyVault/vaults/Kubernetes9330"
                        },
                        "secretName": "K8SPID"
                      }
                },
                "servicePrincipalClientSecret": {
                    "reference": {
                        "keyVault": {
                          "id": "/subscriptions/05499249-2bf5-4767-b121-db90943e363e/resourceGroups/kubernetesrg921115/providers/Microsoft.KeyVault/vaults/Kubernetes9330"
                        },
                        "secretName": "K8SPSecret"
                      }  
                }
              }
            }
        }
    ]
  }
